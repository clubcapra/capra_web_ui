{
  "name": "capra_web_ui",
  "version": "2.0.3",
  "author": {
    "name": "Club Capra",
    "email": "admin@clubcapra.com",
    "url": "http://www.clubcapra.com"
  },
  "description": "User Interface for ROS based robots",
  "private": true,
  "homepage": "./",
  "main": "build/main.js",
  "scripts": {
    "start": "concurrently -k -s first -n renderer,main npm:start:renderer npm:start:main",
    "start:renderer": "parcel src/renderer/index.html --out-dir build -p 3841",
    "start:main": "npm run build:main && wait-on http://localhost:3841 && electron .",
    "build": "npm run build:renderer && npm run build:main && electron-builder",
    "build:main": "parcel build src/main/main.ts --out-dir build --target electron",
    "build:renderer": "parcel build src/renderer/index.html --out-dir build --public-url ./",
    "test": "jest",
    "lint": "eslint --ext js,ts,tsx src",
    "fix": "eslint --fix --ext js,ts,tsx src",
    "typecheck": "tsc --noEmit",
    "check": "concurrently npm:lint npm:typecheck",
    "electron": "electron ."
  },
  "build": {
    "appId": "com.clubcapra.capra_web_ui",
    "files": [
      "build/**/*"
    ],
    "linux": {
      "target": [
        "deb"
      ]
    },
    "directories": {
      "buildResources": ".src/shared/build/resources/"
    },
    "artifactName": "capra_web_ui_setup.${ext}",
    "extends": null
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.3.6",
    "@xstate/react": "^0.8.1",
    "chart.js": "^2.9.3",
    "chartjs-plugin-streaming": "^1.8.0",
    "date-fns": "^2.14.0",
    "lodash": "^4.17.20",
    "polished": "^3.6.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-icons": "^3.10.0",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "react-toastify": "^6.0.5",
    "redux": "^4.0.5",
    "ros3d": "^1.0.1",
    "roslib": "^1.1.0",
    "shortid": "^2.2.15",
    "styled-components": "^5.1.1",
    "xstate": "^4.10.0"
  },
  "devDependencies": {
    "@types/chart.js": "^2.9.21",
    "@types/jest": "^26.0.14",
    "@types/lodash": "^4.14.154",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-dom": "^5.1.5",
    "@types/roslib": "^1.1.0",
    "@types/shortid": "0.0.29",
    "@types/styled-components": "^5.1.3",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^5.2.0",
    "electron": "^10.1.2",
    "electron-builder": "^22.7.0",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.2",
    "jest": "^26.4.2",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.0.5",
    "ts-jest": "^26.4.0",
    "typescript": "^4.0.3",
    "wait-on": "^5.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
